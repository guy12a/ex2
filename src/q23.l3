(define make-error (lambda () 'error))
(define is-error? (lambda (x) (eq? x 'error)))

(define bind
    (lambda (x f)
    (if (is-error? x)
    (make-error)
    (f x)
    ))
)


(define member?
  (lambda (sym lst)
    (((lambda (rec)
    (rec rec sym lst)))
    (lambda (rec k left)
      (if (eq? '() left)
        #f
        (if (eq? (car left) k)
        #t
        (rec rec k (cdr left)))))
    )))


(define unique-keys?
  (lambda (pairs)
    ((lambda (rec)
     (rec rec pairs '()))
    (lambda (rec lst seen)
      (if (eq? lst '())
        #t 
        (if (member? (car (car lst)) seen )
        #f 
        (rec rec (cdr lst) (cons (car (car lst)) seen)))
      )))))


(define dict
  (lambda (lst)
    (if (and (dict? lst)          
             (unique-keys? lst))       
        lst
        (make-error))))

(define get
    (lambda (lst val) 
    (if (not (dict? lst))
        (make-error)
        (if (eq? lst '())
            (make-error)
            (if (eq? (car(car lst)) val)
            (cdr (car lst))
            (get (cdr lst) val)
            )
        )
    ))
)

(define dict?
    (lambda (lst)
      ((lambda (rec) (rec rec lst))
      (lambda (rec pairs) 
        (if (eq? pairs '())
        #t
        (if (pair? pairs)
        (if (pair? (car pairs))
        (rec rec (cdr pairs))
        #f)
        #f
        )
        )
      )
      )
    ))